<Window x:Class="PM.Gui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PM.Gui"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="450"
        Width="800">
    <Grid>
        <DataGrid x:Name="dataGridContactInfo"
                  Margin="516,169,9.6,10">
        </DataGrid>
        <TextBox x:Name="textBoxProjectName"
                 HorizontalAlignment="Left"
                 Height="23"
                 Margin="370,169,0,0"
                 TextWrapping="Wrap"
                 Text=""
                 VerticalAlignment="Top"
                 Width="120" />
        <TextBox x:Name="textBoxDescription"
                 HorizontalAlignment="Left"
                 Height="23"
                 Margin="370,255,0,0"
                 TextWrapping="Wrap"
                 Text=""
                 VerticalAlignment="Top"
                 Width="120" />
        <Button x:Name="buttonSave"
                Content="Button"
                HorizontalAlignment="Left"
                Margin="370,342,0,0"
                VerticalAlignment="Top"
                Width="75"
                Click="buttonSave_Click" />
    </Grid>
</Window>
